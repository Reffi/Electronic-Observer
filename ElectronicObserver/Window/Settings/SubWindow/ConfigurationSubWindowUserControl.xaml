<UserControl
	x:Class="ElectronicObserver.Window.Settings.SubWindow.ConfigurationSubWindowUserControl"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:ElectronicObserver.Window.Settings.SubWindow"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DataContext="{d:DesignInstance local:ConfigurationSubWindowViewModel}"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d"
	>
	<TabControl>
		<TabItem Header="{Binding Translation.Fleet}" />
		<TabItem Header="{Binding Translation.Arsenal}" />
		<TabItem Header="{Binding Translation.Dock}" />
		<TabItem Header="{Binding Translation.HQ}" />
		<TabItem Header="{Binding Translation.Compass}" />
		<TabItem Header="{Binding Translation.Quests}" />
		<TabItem Header="{Binding Translation.Group}" />
		<TabItem Header="{Binding Translation.Combat}" />
		<TabItem Header="{Binding Translation.Browser}" />
		<TabItem Header="{Binding Translation.AB}" />
		<TabItem Header="{Binding Translation.JSON}" />
	</TabControl>
</UserControl>
